
<% if @is_liked %>
$('#like_<%= @review.id%>').html("<i class=\"fa fa-thumbs-up\" aria-hidden=\"true\"></i>");

noty({text: 'Liked', type: 'alert'});

<% else %>

$('#like_<%= @review.id%>').html("<i class=\"fa fa-thumbs-o-up\" aria-hidden=\"true\"></i>");

noty({text: 'Unliked', type: 'alert'});

<% end %>

$('#comment_<%= params[:review] %>').html("<%= Restaurant.where(id: @restaurant).first.like_count @review %>");